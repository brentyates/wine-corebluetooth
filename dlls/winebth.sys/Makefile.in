MODULE  = winebth.sys
IMPORTS = ntoskrnl uuid
UNIXLIB = winebth.so
UNIX_CFLAGS = $(DBUS_CFLAGS) -x objective-c
UNIX_LIBS = $(PTHREAD_LIBS) -framework CoreBluetooth -framework Foundation -framework IOBluetooth -lobjc

EXTRADLLFLAGS = -Wl,--subsystem,native

SOURCES = \
	corebth.m \
	dbus.c \
	unixlib.c \
	winebluetooth.c \
	winebth.c \
	winebth.rc
